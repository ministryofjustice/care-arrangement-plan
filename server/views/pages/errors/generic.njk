{% extends "../../partials/layout.njk" %}

{% block content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <h1>{{ title }}</h1>
      {% if not production %}
        <h2>{{ status }}</h2>
        <pre>{{ stack }}</pre>
      {% endif %}
      <p>The error has been logged.</p>
      <p>You can try reloading the page or <a href="/" class="govuk-link--no-visited-state">start again</a>.</p>
    </div>
  </div>
{% endblock %}
